{
  "name": "skeleton-react-express-with-typescript",
  "version": "1.0.0",
  "description": "Skeleton for React and Express with TypeScript",
  "scripts": {
    "start": "cross-env NODE_ENV=production concurrently \"ts-node src/server/main.ts\" \"vite --host\"",
    "dev:server": "cross-env NODE_ENV=development nodemon src/server/main.ts",
    "dev:client": "cross-env NODE_ENV=development vite"
  },
  "keywords": [
    "Skeleton",
    "React",
    "Express",
    "TypeScript"
  ],
  "author": "DarkMalk",
  "license": "ISC",
  "dependencies": {
    "concurrently": "8.0.1",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "morgan": "1.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.10.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/morgan": "1.9.4",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "nodemon": "2.0.22",
    "ts-node": "10.9.1",
    "typescript": "5.0.3",
    "vite": "4.2.1"
  }
}
